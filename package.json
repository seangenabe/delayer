{
  "name": "delayer",
  "version": "1.0.1",
  "description": "Create a cancellable promise that will be resolved in a specified amount of time.",
  "main": "dist/delayer.js",
  "bin": {
    "delayer": "./bin/delayer"
  },
  "scripts": {
    "start": "node .",
    "pretest": "npm run compile",
    "test": "ava",
    "compile": "babel lib --out-dir dist",
    "watch": "npm run compile -- --watch",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seangenabe/delayer"
  },
  "keywords": [
    "promise",
    "delay",
    "timeout",
    "cli"
  ],
  "author": "Sean Genabe <seangenabe@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seangenabe/delayer/issues"
  },
  "homepage": "https://github.com/seangenabe/delayer",
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-async-to-generator": "^6.5.0"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator"
    ],
    "sourceMaps": true
  },
  "engines": {
    "node": ">=4"
  }
}
